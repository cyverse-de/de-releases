base_url: {{ de_base_uri.rtrim(pat="/")}}
terrain_url: {{ baseurls_terrain }}
user_portal_url: {{ user_portal_base_uri.rtrim(pat="/")}}
support_email: {{ email_support_dest }}
de_faq: {{ de_info_faq }}
cyverse_url: {{ de_info_description }}
db:
  host: {{ groups['dbms_primary'][0] }}
  user: {{ dbms_connection_user }}
  password: {{ dbms_connection_pass }}
keycloak:
  server_url: {{ keycloak_server_uri }}
  realm: {{ keycloak_realm_name }}
  client_id: {{ keycloak_client_id }}
  client_secret: {{ keycloak_client_secret }}
intercom:
  app_id: {{ intercom_app_id }}
  enabled: {{ intercom_enabled }}
  company_id: {{ intercom_company_id }}
  company_name: {{ intercom_company_name }}
  user_profile_url: https://app.intercom.com/apps/{{ intercom_app_id }}/users/show?user_id=
irods:
  home_path: /{{ irods_zone }}/home
  trash_path: /{{ irods_zone }}/trash/home
  community_path: /{{ irods_zone }}/home/shared
amqp:
  amqp_uri: amqp://{{ de_amqp_user }}:{{ de_amqp_password | urlencode }}@{{ de_amqp_host }}:{{ de_amqp_port }}/{{ de_amqp_vhost | urlencode }}
  exchange_name: de
  exchange_durable: true
  auto_delete: false
admin:
  group_attribute_name: {{ Admin.Attribute }}
  groups: {{ Admin.Groups }}
fileIdentifier:
  htPathList: "# application/vnd.de.path-list+csv; version=1"
  multiInputPathList: "# application/vnd.de.multi-input-path-list+csv; version=1"
analytics:
  enabled: {{ analytics_enabled }}
  id: {{ analytics_id }}
grouper:
  allUsers: GrouperAll
  admin: de_grouper
tools:
  admin:
    max_cpu_limit: {{ de_tools_admin_max_cpu_limit }}
    max_memory_limit: {{ de_tools_admin_max_memory_limit }}
    max_disk_limit: {{ DE.Tools.Admin.MaxDiskLimit }}
unleash:
  api_url: {{ unleash_base_url.rtrim(pat="/")}}{{ Unleash.APIPath }}
  client_secret: "{{ unleash_api_token }}"
  maintenance_flag: {{ unleash_maintenance_flag }}
username:
  suffix: {{ uid_domain }}
subscriptions:
  checkout_url: {{ de_subscriptions_checkout_url.rtrim(pat="/")}}
  enforce: {{ de_subscriptions_enforce }}
elastic:
  enabled: {{ es_enabled }}
